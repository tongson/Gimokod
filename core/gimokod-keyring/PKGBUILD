pkgname=gimokod-keyring
pkgver=2011.10
pkgrel=1
pkgdesc="Gimokod Linux keyring"
arch=('any')
url="http://gimokod.org"
license=('LGPL3')
groups=('base')
install=gimokod-keyring.install

package() {
  install -D -m0644 gpg.conf "${pkgdir}/usr/share/pacman/gpg.conf"
  install -D -m0644 gimokod-keyring.gpg "${pkgdir}/usr/share/pacman/gimokod-keyring.gpg"
  install -D -m0644 gimokod-trustdb.gpg "${pkgdir}/usr/share/pacman/gimokod-trustdb.gpg"
}

source=(${source[@]-}
        gimokod-keyring.gpg
        gimokod-trustdb.gpg
        gpg.conf)

sha256sums=(${sha256sums[@]-}
            '6be165d0c4fa7774904e6742cb5ce2dfc0ad3adced7108eb5f3b929a465cf9e9'
            '2c9d47930cb29c3b801f577cf4b04ac6f9d43d50c91876b3aeacbb91af12e108'
            '0cbac937bd7ef2755ec501e87d58b5c19d825beef690e1a9bedd60d8463d0288')
