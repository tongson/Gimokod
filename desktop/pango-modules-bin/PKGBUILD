pkgname=pango-modules-bin
pkgver=1.6.0
pkgrel=1
pkgdesc="Pango modules (binary)"
arch=('x86_64')
license=('LGPL')
depends=('pango')
install=pango.install
url="http://www.pango.org/"
source=(http://localhost/${pkgname}-${pkgver}.tar.xz)
sha256sums=('2114b5226870aa8bab876b3cce8eb69dc7d7d42e9d21b6d6c34619d5b9d0df4f')
noextract=("${pkgname}-${pkgver}.tar.xz")

package() {
  pushd "${pkgdir}"
    tar -xJf "${srcdir}"/"${pkgname}-${pkgver}.tar.xz"
  popd
}
